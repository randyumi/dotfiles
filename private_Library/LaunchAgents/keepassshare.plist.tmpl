<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>keepassdbshare-check</string>
  <key>EnableGlobbing</key>
  <true/>
  <key>ProgramArguments</key>
  <array>
    <string>{{- .chezmoi.homeDir}}/bin/keepassdbshare</string>
    <string>check</string>
  </array>
  <key>StartCalendarInterval</key>
  <array>
    <dict>
      <key>Minute</key>
      <integer>0</integer>
    </dict>
    <dict>
      <key>Minute</key>
      <integer>30</integer>
    </dict>
  </array>
  <key>RunAtLoad</key>
  <true />
</dict>
</plist>

